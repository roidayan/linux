@@ -95,7 +103,11 @@ static ctl_table rds_sysctl_rds_table[] = {
 void rds_sysctl_exit(void)
 {
 	if (rds_sysctl_reg_table)
+#if LINUX_VERSION_CODE >= KERNEL_VERSION(3,5,0)
